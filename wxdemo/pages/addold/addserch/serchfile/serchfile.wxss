.page_box{
  width: 100%;
}
.page_header{
  width: 100%;
  height: 200rpx;
  position: relative;
}
.header_img{
position: absolute;
width: 80rpx;
height:80rpx;
top: 60rpx;
left:200rpx;

}
.header_img>image{
  width: 80rpx;
  height: 80rpx;
}
.name_box{
  font-family: "微软雅黑";
  font-size: 25px;
  letter-spacing: 2px;
}
.header_name_box{
  position: absolute;
  top: 65rpx;
  left: 350rpx;
  letter-spacing: 2px;
}
/*总体*/
.people_file_box{
  width: 100%;
} 
.file_list{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom:1px solid #CCCC;
}
.text_left{
  float: left;
  margin-left: 35rpx;
  font-size: 14px;
}
.text_right{
 float: right;
 margin-right: 35rpx;
 font-size: 14px;
}
.list_a{
  border-top: 1px solid rgba(0,0,0,0.1);
}
.section {
  width: 100%;
}
.section button {
  width: 60%;
  height: 85rpx;
  padding: 0;
  margin: 50rpx auto;
  font-size: 36rpx;
  color: #fff;
  line-height: 85rpx;
  box-sizing: border-box;
  border: none;
  border-radius: 40rpx;
  background:rgb(64, 211, 164);
cursor: pointer;
}